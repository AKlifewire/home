tamp: AWSDateTime\n}\n\ntype AnalyticsResult {\n  success: Boolean!\n  data: AnalyticsData\n}\n\ntype AnalyticsData {\n  deviceId: ID\n  metrics: [MetricPoint]\n  insights: [String]\n}\n\ntype UiJsonResult @aws_api_key @aws_cognito_user_pools {\n  success: Boolean! @aws_api_key @aws_cognito_user_pools\n  data: String @aws_api_key @aws_cognito_user_pools\n  message: String @aws_api_key @aws_cognito_user_pools\n}\n\ntype MetricPoint {\n  timestamp: AWSDateTime\n  value: Float\n  type: String\n}"
   },
